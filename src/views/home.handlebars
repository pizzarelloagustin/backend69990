<h1>Todos los productos</h1>

<div class="ctnProducts">
    {{#each products}}
        <div class="innerProduct">
            <p> {{this.title}} </p>
            <p> {{this.description}} </p>
            <p> {{this.code}} </p>
            <p> {{this.price}} </p>
            <p> {{this.status}} </p>
            <p> {{this.stock}} </p>
            <p> {{this.category}} </p>
            <p> {{this.thumbnails}} </p>
        </div>
    {{/each}}
</div>